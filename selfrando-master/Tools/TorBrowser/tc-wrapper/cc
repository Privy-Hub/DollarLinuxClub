set -ue

exec 2> >(tee -a /tmp/tc.log >&2)

TC_PATH="$(readlink -e $(dirname "$0"))"
source $TC_PATH/functions.sh

set -x
run_original -B"$TC_PATH" -ffunction-sections -fdata-sections -fPIC "$@"
